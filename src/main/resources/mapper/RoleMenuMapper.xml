<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leis.bear.mapper.RoleMenuMapper">
  <resultMap id="BaseResultMap" type="com.leis.bear.domain.RoleMenu">
    <result column="menu_id" jdbcType="INTEGER" property="menuId" />
    <result column="role_id" jdbcType="INTEGER" property="roleId" />
  </resultMap>
  <insert id="insert" parameterType="com.leis.bear.domain.RoleMenu">
    insert into t_role_menu (menu_id, role_id)
    values (#{menuId,jdbcType=INTEGER}, #{roleId,jdbcType=INTEGER})
  </insert>
  <select id="selectAll" resultMap="BaseResultMap">
    select menu_id, role_id
    from t_role_menu
  </select>
</mapper>